{
  "version": 2,
  "buildCommand": "echo 'Static HTML site - no build needed'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/service",
      "destination": "/api/server.js"
    }
  ]
}
